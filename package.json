{
  "type": "commonjs",
  "version": "1.0.0",
  "name": "my-webpack-project",
  "description": "My webpack project",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --config webpack.client.ts",
    "build:server": "webpack --config webpack.server.ts",
    "start": "npm run build && node dist/server/index.js",
    "dev": "webpack serve --config webpack.dev.ts ",
    "dev:server": "babel-node --extensions '.tsx,.ts,.js' src/server/index.ts"
  },
  "nodemonConfig": {
    "exec": "npm run build && node dist/server/index.js --dev",
    "ext": "ts,tsx",
    "ignore": [
      "**/dist/**",
      "**/node_modules/**",
      "src/client/**"
    ],
    "watch": "src/server"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fastify/cors": "^8.3.0",
    "@fastify/static": "^6.10.2",
    "@reduxjs/toolkit": "^1.9.5",
    "fastify": "^4.18.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.12.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/node": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@babel/register": "^7.22.5",
    "@emotion/babel-plugin": "^11.11.0",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "html-webpack-plugin": "^5.5.3",
    "nodemon": "^2.0.22",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.6",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
